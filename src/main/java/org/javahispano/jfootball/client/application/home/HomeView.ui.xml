<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:my="urn:import:org.javahispano.jfootball.client.application.widget.viewmatch">
	<b:Container>
		<b:Form type="HORIZONTAL">
			<b:FieldSet>
				<b:Legend>Ejemplo compilación y ejecución en GAE</b:Legend>

				<b:FormGroup>
					<b:FormLabel for="formCode" addStyleNames="col-lg-2">Código</b:FormLabel>
					<g:FlowPanel addStyleNames="col-lg-10" ui:field="flowPanel">
					</g:FlowPanel>
				</b:FormGroup>

				<b:FormGroup>
					<g:FlowPanel addStyleNames="col-lg-offset-2 col-lg-10">
						<b:Button ui:field="send">Enviar</b:Button>
					</g:FlowPanel>
				</b:FormGroup>
			</b:FieldSet>
		</b:Form>
		<b:Lead text="Resultado de la compilación / ejecución"></b:Lead>
		<b.html:Paragraph ui:field="result"></b.html:Paragraph>
		<b:Button text="Comenzar" ui:field="start"></b:Button>
		<my:MainWidget ui:field="mainWidget"></my:MainWidget>
	</b:Container>
</ui:UiBinder>
